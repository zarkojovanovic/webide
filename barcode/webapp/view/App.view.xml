<mvc:View controllerName="com.barcode.barcode.controller.App" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<Button text="Scan" press="onPress" icon="sap-icon://bar-code"/>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>